ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name reasongl-interface
src_root_dir = /Users/benjamin/Desktop/reprocessing/node_modules/reasongl-interface
bsc = /Users/benjamin/Desktop/bucklescript/jscomp/bin/bsc.exe
bsb_helper = /Users/benjamin/Desktop/bucklescript/jscomp/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I /Users/benjamin/Desktop/reprocessing/node_modules/bs-platform/lib/ocaml -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = 
external_deps_for_linking = 
refmt = /Users/benjamin/Desktop/bucklescript/jscomp/bin/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
ocamlc = ocamlc
ocamlopt = ocamlopt
bsb_dir_group = 0
bsc_lib_includes = -I src
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/RGLEvents.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/RGLEvents.re
rule build_deps
  command = ${bsb_helper} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/RGLEvents.mlast.d : build_deps src/RGLEvents.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/RGLEvents.cmj $src_root_dir/lib/js/src/RGLEvents.js src/RGLEvents.cmi : build_cmj_cmi src/RGLEvents.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/RGLConstants.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/RGLConstants.re
build src/RGLConstants.mlast.d : build_deps src/RGLConstants.mlast
build src/RGLConstants.cmj $src_root_dir/lib/js/src/RGLConstants.js src/RGLConstants.cmi : build_cmj_cmi src/RGLConstants.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/RGLInterface.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/RGLInterface.re
build src/RGLInterface.mlast.d : build_deps src/RGLInterface.mlast
build src/RGLInterface.cmj $src_root_dir/lib/js/src/RGLInterface.js src/RGLInterface.cmi : build_cmj_cmi src/RGLInterface.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build build.ninja : phony  ||  src/RGLInterface.mlast.d src/RGLConstants.mlast.d src/RGLEvents.mlast.d
